package main

import . "fmt"

func main() {
	a := [4]string{"test", "123", "simpletest", "abc"}[1]
	b := [4]string{"test", "123", "simpletest", "abc"}[2]
	c := [4]string{"test", "123", "simpletest", "abc"}[3]
	d := [...]string{"test", "123", "simpletest", "abc"}
	Println(a)
	Println(b)
	Println(c)
	d[3] = "erf"
	Println(d)
	d[4] = "hjk"
	Println(d) // error
}
